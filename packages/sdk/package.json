{
  "name": "@luneo/sdk",
  "version": "1.0.0",
  "description": "Official Luneo JavaScript/TypeScript SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "luneo",
    "sdk",
    "api",
    "product-customization",
    "design",
    "ecommerce"
  ],
  "author": "Luneo <dev@luneo.app>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luneo/sdk-js"
  },
  "homepage": "https://luneo.app/developers",
  "bugs": {
    "url": "https://github.com/luneo/sdk-js/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}


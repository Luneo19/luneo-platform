{
  "version": 2,
  "buildCommand": "cd apps/frontend && bash vercel-build.sh",
  "installCommand": "echo 'Installation gérée par build script'",
  "outputDirectory": "apps/frontend/.next",
  "framework": null,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD apps/frontend/",
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/api/:path*"
    }
  ]
}


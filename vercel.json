{
  "framework": "nextjs",
  "buildCommand": "cd apps/frontend && NODE_OPTIONS=--max_old_space_size=4096 pnpm run build",
  "outputDirectory": "apps/frontend/.next",
  "installCommand": "pnpm install --ignore-scripts && cd apps/frontend && npx prisma generate",
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.luneo.app/api/:path*"
    }
  ]
}

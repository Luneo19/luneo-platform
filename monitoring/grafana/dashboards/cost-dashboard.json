{
  "dashboard": {
    "title": "LUNEO - Cost Dashboard",
    "tags": ["cost", "billing", "ai"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Total AI Costs (Last 30 Days)",
        "type": "stat",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "sum(increase(ai_costs_cents_total[30d])) / 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 100, "color": "yellow" },
                { "value": 500, "color": "red" }
              ]
            },
            "unit": "currencyUSD",
            "decimals": 2
          }
        }
      },
      {
        "id": 2,
        "title": "AI Costs by Provider",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "sum(increase(ai_costs_cents_total[30d])) by (provider)",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 3,
        "title": "AI Costs by Tenant",
        "type": "bargauge",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "topk(10, sum(increase(ai_costs_cents_total[30d])) by (brandId))",
            "legendFormat": "{{brandId}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2
          }
        }
      },
      {
        "id": 4,
        "title": "AI Costs Trend",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "sum(rate(ai_costs_cents_total[5m])) by (provider)",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "currencyUSD",
            "label": "Cost per second"
          }
        ]
      },
      {
        "id": 5,
        "title": "AI Generations by Model",
        "type": "table",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "sum(increase(ai_generations_total[30d])) by (model, provider)",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "model": "Model",
                "provider": "Provider",
                "Value": "Generations"
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}






















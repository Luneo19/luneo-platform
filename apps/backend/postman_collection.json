              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"alice@ex.com\",\n  \"password\": \"SecureP@ssw0rd\",\n  \"firstName\": \"Alice\",\n  \"lastName\": \"Dupont\",\n  \"role\": \"CONSUMER\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}{{apiPrefix}}/auth/signup",
              "host": ["{{baseUrl}}"],
              "path": ["{{apiPrefix}}", "auth", "signup"]
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@luneo.com\",\n  \"password\": \"admin123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}{{apiPrefix}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["{{apiPrefix}}", "auth", "login"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', response.data.accessToken);",
                  "    pm.collectionVariables.set('refreshToken', response.data.refreshToken);",
                  "}"
                ]
              }
            }
          ]
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refreshToken\": \"{{refreshToken}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}{{apiPrefix}}/auth/refresh",
              "host": ["{{baseUrl}}"],
              "path": ["{{apiPrefix}}", "auth", "refresh"]
            }
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refreshToken\": \"{{refreshToken}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}{{apiPrefix}}/auth/logout",
              "host": ["{{baseUrl}}"],
              "path": ["{{apiPrefix}}", "auth", "logout"]

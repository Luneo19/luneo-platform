# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/luneo_production"

# Redis
REDIS_URL="redis://localhost:6379"

# Server
NODE_ENV="development"
PORT=4000

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# SMTP Configuration (CORRIGÉ - valeurs booléennes sans guillemets)
SMTP_SECURE=false
DOMAIN_VERIFIED=false

# Email
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER=""
SMTP_PASS=""

# OAuth
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
GITHUB_CLIENT_ID=""
GITHUB_CLIENT_SECRET=""

# AI
OPENAI_API_KEY=""

# Stripe
STRIPE_SECRET_KEY=""
STRIPE_WEBHOOK_SECRET=""

# File Upload
MAX_FILE_SIZE="10mb"

# CORS
CORS_ORIGIN="http://localhost:3000"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

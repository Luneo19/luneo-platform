{
  "version": 2,
  "installCommand": "npm install --include=dev",
  "buildCommand": "npx prisma generate && npm run build",
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  },
  "outputDirectory": "dist"
}
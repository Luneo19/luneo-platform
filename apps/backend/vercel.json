{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "cd ../.. && pnpm install --filter @luneo/backend-vercel... --frozen-lockfile",
  "buildCommand": "cd ../.. && pnpm --filter @luneo/backend-vercel... run build"
}
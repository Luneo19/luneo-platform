{
  "version": 2,
  "installCommand": "npm install --include=dev",
  "buildCommand": "npx prisma generate && npm run build && cp tsconfig.json dist/",
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  },
  "outputDirectory": "dist"
}
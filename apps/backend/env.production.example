# Configuration de Production - Luneo Backend
NODE_ENV=production
PORT=3001

# Base de données
DATABASE_URL=postgresql://luneo_user:luneo_password_2025@localhost:5432/luneo_enterprise

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=luneo_jwt_secret_2025_production_secure
JWT_REFRESH_SECRET=luneo_refresh_secret_2025_production_secure

# OAuth - À configurer avec vos vraies clés
GOOGLE_CLIENT_ID=your_google_client_id

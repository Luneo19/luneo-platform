  "main": "api/index.js",
  "scripts": {
    "build": "nest build --tsc -p tsconfig.build.json",
    "vercel-build": "nest build",
    "postinstall": "prisma generate || true",
    "start": "node dist/src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathPattern=src/",
    "test:integration": "RUN_INTEGRATION_TESTS=true jest --testPathPattern=test/",
    "test:integration:setup": "docker-compose -f docker-compose.test.yml up -d && npx prisma migrate deploy",
    "test:integration:teardown": "docker-compose -f docker-compose.test.yml down",
    "test:e2e": "pnpm run test:integration:setup && pnpm run test:integration && pnpm run test:integration:teardown",
    "test:mutation": "stryker run stryker.conf.json",
    "test:mutation:auth": "stryker run stryker.conf.json --mutate src/modules/auth/**/*.ts"
  },
